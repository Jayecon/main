* Encoding: UTF-8.
DEFINE !StateLoop (!POSITIONAL  !CHAREND('/') 
                                / !POSITIONAL  !CMDEND) 
!DO !I !IN (!1) 
 !DO !J !IN (!2) 
    !LET !F = !QUOTE(!CONCAT("C:\TIMSS\TIMSS07\T07_SPSS_G8\",!I,!J,"m4.sav"))
    !LET !G = !QUOTE(!CONCAT("C:\Users\osjle\Dropbox\Stata\TIMSS\Stata_original\",!I,!J,"m4.dta"))
    GET FILE = !F.
    SAVE TRANSLATE OUTFILE= !G
          /TYPE=STATA
          /VERSION=13
          /EDITION=SE
          /MAP
          /REPLACE.
 !DOEND 
!DOEND 
!ENDDEFINE. 

